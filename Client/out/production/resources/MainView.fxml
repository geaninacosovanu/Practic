<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <top>
      <AnchorPane prefHeight="65.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="buttonLogout" layoutX="504.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#handleLogoutBotton" text="Logout" />
         </children>
      </AnchorPane>
   </top>
   <center>
      <AnchorPane prefHeight="353.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="buttonStart" layoutX="505.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#handlebuttonStart" text="Start" />
            <ListView fx:id="listViewParticipanti" layoutX="22.0" layoutY="14.0" prefHeight="117.0" prefWidth="192.0" />
            <Label fx:id="labelNumere" layoutX="265.0" layoutY="18.0" prefHeight="17.0" prefWidth="94.0" />
            <TextField fx:id="textFieldNumar" layoutX="435.0" layoutY="73.0" prefHeight="25.0" prefWidth="111.0" />
            <Button fx:id="buttonAdd" layoutX="446.0" layoutY="119.0" mnemonicParsing="false" onMouseClicked="#handleAdd" text="Add" />
         </children></AnchorPane>
   </center>
</BorderPane>
