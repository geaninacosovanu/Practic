            <Button fx:id="buttonLogout" layoutX="504.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#handleLogoutBotton" text="Logout" />
            <Label fx:id="labelId" layoutX="42.0" layoutY="24.0" prefHeight="17.0" prefWidth="86.0" />
         </children>
      </AnchorPane>
   </top>
   <center>
      <AnchorPane prefHeight="353.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="buttonStart" layoutX="506.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#handleStart" text="Start" />
         </children></AnchorPane>
   </center>
</BorderPane>
