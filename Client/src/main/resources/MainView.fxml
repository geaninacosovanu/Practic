<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tableViewParticipanti" layoutX="83.0" layoutY="81.0" prefHeight="200.0" prefWidth="226.0">
              <columns>
                <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="Id" />
                <TableColumn fx:id="tableColumnNume" prefWidth="75.0" text="Nume" />
                  <TableColumn fx:id="tableColumnStatus" prefWidth="75.0" text="Status" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <ComboBox fx:id="comboBoxParticipant" layoutX="419.0" layoutY="81.0" prefWidth="150.0" />
            <Label layoutX="347.0" layoutY="125.0" text="Nota" />
            <Label layoutX="347.0" layoutY="85.0" text="Participant" />
            <TextField fx:id="textFieldNota" layoutX="420.0" layoutY="130.0" />
            <Button fx:id="buttonAdd" layoutX="442.0" layoutY="181.0" mnemonicParsing="false" onMouseClicked="#handleAddButtonClick" text="Add" />
            <Button fx:id="buttonLogout" layoutX="494.0" layoutY="31.0" mnemonicParsing="false" onMouseClicked="#handleLogoutBotton" text="Logout" />
            <Label fx:id="labelAspect" layoutX="83.0" layoutY="35.0" text="Label" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
