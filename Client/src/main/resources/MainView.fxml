<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <top>
      <AnchorPane prefHeight="65.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="buttonLogout" layoutX="504.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#handleLogoutBotton" text="Logout" />
            <Label fx:id="labelPunctControl" layoutX="26.0" layoutY="24.0" text="Label" />
         </children>
      </AnchorPane>
   </top>
   <center>
      <AnchorPane prefHeight="353.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="tableView" layoutX="21.0" layoutY="21.0" prefHeight="201.0" prefWidth="361.0">
              <columns>
                <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="Id" />
                <TableColumn fx:id="tableColumnNume" prefWidth="75.0" text="Nume" />
                  <TableColumn fx:id="tableColumnPunctControl" prefWidth="75.0" text="PunctControl" />
                  <TableColumn fx:id="tableColumnOra" prefWidth="75.0" text="Ora" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <ComboBox fx:id="comboCopil" layoutX="440.0" layoutY="44.0" prefHeight="25.0" prefWidth="109.0" />
            <TextField fx:id="textFieldOra" layoutX="452.0" layoutY="9.0" prefHeight="25.0" prefWidth="28.0" />
            <TextField fx:id="textFieldMinute" layoutX="502.0" layoutY="9.0" prefHeight="25.0" prefWidth="27.0" />
            <Label layoutX="491.0" layoutY="13.0" text=":" />
            <Button fx:id="buttonAdd" layoutX="467.0" layoutY="89.0" mnemonicParsing="false" onMouseClicked="#handleAddButton" text="Add" />
         </children></AnchorPane>
   </center>
</BorderPane>
