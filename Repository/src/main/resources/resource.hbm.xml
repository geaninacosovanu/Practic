<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Joc" table="Joc">
        <id name="id">
            <column name="Id"/>
            <generator class="increment"/>
        </id>
        <property name="castigator" column="Castigator"/>
    </class>
</hibernate-mapping>